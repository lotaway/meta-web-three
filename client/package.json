{
  "name": "meta-web-three-client",
  "version": "1.0.0",
  "type": "module",
  "homepage": "/index.html",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "build:wasm": "cd ../wasm-ff & wasm-pack build --scope lotaway",
    "build:wasm:debug": "cd ../wasm-ff & cargo build --target wasm32-unknown-unknown & wasm-bindgen --keep-debug --out-dir pkg ./target/wasm32-unknown-unknown/debug/Wasm_FF.wasm",
    "build:full": "npm run build:wasm & npm run build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@farmfe/js-plugin-svgr": "^1.4.0",
    "@farmfe/plugin-sass": "^1.0.5",
    "@web3modal/ethers": "^3.5.2",
    "@web3modal/wallet": "^3.5.2",
    "gl-matrix": "^3.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^5.3.1"
  },
  "devDependencies": {
    "@farmfe/cli": "^1.0.2",
    "@farmfe/core": "^1.1.14",
    "@farmfe/plugin-react": "^1.1.0",
    "@reduxjs/toolkit": "^1.9.2",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/redux": "^3.6.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@webGPU/types": "npm:@webgpu/types@^0.1.34",
    "autoprefixer": "^10.4.13",
    "buffer": "^6.0.3",
