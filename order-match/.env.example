# Order Match Service Environment Variables
# Copy this file to .env and modify values as needed

# Application
APP_NAME=order-match
LOG_LEVEL=info

# Kafka Configuration
KAFKA_BROKERS=localhost:9092
KAFKA_TOPIC=dex-trades
KAFKA_PRODUCER_TIMEOUT_MS=5000
KAFKA_PRODUCER_RETRY_COUNT=3

# Dubbo Configuration
DUBBO_PORT=20086
DUBBO_REGISTRY_ADDRESS=127.0.0.1:2181
DUBBO_GROUP=/dev/metawebthree
DUBBO_VERSION=

# Storage Configuration
STORAGE_WAL_DIR=/tmp

# Markets Configuration (comma-separated)
MARKETS=BTC/USDT,ETH/USDT,DOGE/USDT
